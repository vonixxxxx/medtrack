dist/
build/
coverage/
.nyc_output/
.cache/
.next/
out/

# Logs
logs/
*.log
*.log.*

# Python files
__pycache__/
*.pyc
*.py
*.pth
*.whl
*.egg-info/

# Database files
*.sql
*.db
*.sqlite
*.sqlite3

# IDE files
.vscode/
.idea/
*.swp
*.swo
*.sublime-*

# OS files
.DS_Store
Thumbs.db
desktop.ini

# Temporary files
temp/
tmp/
